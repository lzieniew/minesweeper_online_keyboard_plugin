{
  "manifest_version": 2,
  "name": "Minesweeper Keyboard Control",
  "version": "1.2",
  "description": "Control Minesweeper game with keyboard.",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": ["*://minesweeper.online/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
